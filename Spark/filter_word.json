{
    "version": "strict-v1",
    "normalize": {
        "lowercase": true,
        "collapse_spaces": true,
        "trim": true
    },
    "filters": [
        {
            "category": "goodbye",
            "priority": 1,
            "strict": {
                "exact": [
                    "안녕히계세요",
                    "안녕히 가세요",
                    "안녕히가세요",
                    "잘자",
                    "잘 자",
                    "잘가",
                    "잘 가",
                    "잘가요",
                    "잘 가요",
                    "굿나잇",
                    "굿밤",
                    "굳나잇",
                    "바이",
                    "바이바이",
                    "bye",
                    "bye bye",
                    "goodbye",
                    "good bye",
                    "see you",
                    "see ya",
                    "see u",
                    "good night",
                    "goodnight",
                    "gtg",
                    "g2g",
                    "i'm out",
                    "im out"
                ],
                "allowed_trailers": [
                    ".",
                    "!",
                    "~",
                    "…",
                    "...",
                    "👋"
                ]
            }
        },
        {
